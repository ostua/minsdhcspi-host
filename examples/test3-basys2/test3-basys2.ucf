# This file is a general .ucf for Basys2 rev C board

# clock pin for Basys2 Board
NET "clk" LOC = "B8"; # Bank = 0, Signal name = MCLK
NET "clk" CLOCK_DEDICATED_ROUTE = FALSE;

NET "btn_in<3>" LOC = "A7";  # Bank = 1, Signal name = BTN3
NET "btn_in<2>" LOC = "M4";  # Bank = 0, Signal name = BTN2
NET "btn_in<1>" LOC = "C11"; # Bank = 2, Signal name = BTN1
NET "btn_in<0>" LOC = "G12"; # Bank = 0, Signal name = BTN0


# Pin assignment for LEDs
NET "Leds_out<7>" LOC = "G1" ; # Bank = 3, Signal name = LD7
NET "Leds_out<6>" LOC = "P4" ; # Bank = 2, Signal name = LD6
NET "Leds_out<5>" LOC = "N4" ;  # Bank = 2, Signal name = LD5
NET "Leds_out<4>" LOC = "N5" ;  # Bank = 2, Signal name = LD4
NET "Leds_out<3>" LOC = "P6" ; # Bank = 2, Signal name = LD3
NET "Leds_out<2>" LOC = "P7" ; # Bank = 3, Signal name = LD2
NET "Leds_out<1>" LOC = "M11" ; # Bank = 2, Signal name = LD1
NET "Leds_out<0>" LOC = "M5" ;  # Bank = 2, Signal name = LD0

#SPI connection for PMODSD
NET "SS" LOC = "C6" ;  # JB
NET "MOSI" LOC = "B6" ;  # JB
NET "SCLK" LOC = "B7" ;  # JB
NET "MISO" LOC = "C5" ;  # JB



# Pin assignment for DispCtl
# Connected to Basys2 onBoard 7seg display
NET "seg_out<0>" LOC = "L14"; # Bank = 1, Signal name = CA
NET "seg_out<1>" LOC = "H12"; # Bank = 1, Signal name = CB
NET "seg_out<2>" LOC = "N14"; # Bank = 1, Signal name = CC
NET "seg_out<3>" LOC = "N11"; # Bank = 2, Signal name = CD
NET "seg_out<4>" LOC = "P12"; # Bank = 2, Signal name = CE
NET "seg_out<5>" LOC = "L13"; # Bank = 1, Signal name = CF
NET "seg_out<6>" LOC = "M12"; # Bank = 1, Signal name = CG
NET "dp_out" LOC = "N13"; # Bank = 1, Signal name = DP

NET "an_out<3>" LOC = "K14"; # Bank = 1, Signal name = AN3
NET "an_out<2>" LOC = "M13"; # Bank = 1, Signal name = AN2
NET "an_out<1>" LOC = "J12"; # Bank = 1, Signal name = AN1
NET "an_out<0>" LOC = "F12"; # Bank = 1, Signal name = AN0

